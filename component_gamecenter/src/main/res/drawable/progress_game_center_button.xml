<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <stroke android:width="0.5dp" android:color="@color/game_center_button_progress"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="100dp"/>
                <solid android:color="@color/game_center_button_progress_20"/>
            </shape>
        </clip>
    </item>
</layer-list>
